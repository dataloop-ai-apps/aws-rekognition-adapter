{
  "displayName": "aws models",
  "name": "aws-models",
  "icon": "icon-dl-qa-sampling",
  "components": {
    "panels": [
      {
        "name": "contentModerationNodePanel",
        "icon": "icon-dl-qa-sampling",
        "supportedSlots": [
          {
            "type": "pipelineNodeConfig",
            "configuration": {}
          }
        ],
        "conditions": {
          "resources": []
        }
      }
    ],
    "pipelineNodes": [
      {
        "invoke": {
          "type": "function",
          "namespace": "contentmoderationnode.content_moderation_module.detect_moderation_labels"
        },
        "categories": [
          "image-utils"
        ]
      }
    ],
    "modules": [
      {
        "name": "content_moderation_module",
        "entryPoint": "imageContentModeration/image_content_moderation.py",
        "className": "ServiceRunner",
        "initInputs": [
          {
            "type": "String",
            "name": "aws_key_name"
          },
          {
            "type": "String",
            "name": "aws_secret_name"
          }
        ],
        "functions": [
          {
            "name": "detect_moderation_labels",
            "description": "aws content moderation",
            "input": [
              {
                "type": "Item",
                "name": "item"
              }
            ],
            "displayIcon": "qa-sampling",
            "displayName": "aws content moderation detection"
          }
        ]
      }
